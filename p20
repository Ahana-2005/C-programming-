//Write a c program to take user input for elements and display them all.
#include <stdio.h>
int main() 
{
    int size;
    printf("Enter size:");
    scanf("%d",&size);
    int numbers[size];
    for(int i=0;i<size;i++)
    {
        printf("Enter the index element:%d\n");
        scanf("%d",&numbers[i]);
    }
    for(int i=0;i<size;i++)
    {
        printf("%d\n",numbers[i]);
    }
}
